.heart {
  position: absolute;
  top: 200px;
  left: 200px;
  width: 40px;
  height: 40px;
  animation: beat3 0.2s ease;
}

.heart-center {
  position: absolute;
  top: 200px;
  left: 200px;
  width: 40px;
  height: 40px;
  animation: beat2 0.2s ease;
}

.heart-active {
  position: absolute;
  top: 200px;
  left: 200px;
  z-index: 2;
  animation: beat 0.5s ease;
  width: 40px;
  height: 40px;
}

@keyframes beat {
  0% {
    transform: scale(1);
  }

  40% {
    transform: scale(0);
  }

  80% {
    transform: scale(1.1);
  }

  100% {
    transform: scale(1);
  }
}

@keyframes beat2 {
  0% {
    transform: scale(1);
  }

  100% {
    transform: scale(0.7);
  }
}

@keyframes beat3 {
  0% {
    transform: scale(0.7);
  }

  100% {
    transform: scale(1);
  }
}

