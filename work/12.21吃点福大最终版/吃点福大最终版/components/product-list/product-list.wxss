.product-list{
    height: 140px;
    margin: 12px;
    padding: 5px;
    border: 1px solid #cccccc;
    border-radius: 10px;
    box-shadow: 0 2px 12px 0 rgb(0 0 0 / 10%);
}
.list_item{
    display: flex;
    justify-content: space-between;
}
.item_img{
    width: 80px;
    height: 80px;
    padding: 5px;
}
.list_item_details{
    flex: 1;
    display: flex;
    justify-content: space-between;
    padding: 0 5px;
    padding-top: 10px;
}
.details_text .title{
    /* font-size: 18px; */
    font-size: 28rpx;
    font-weight: 400;
    color: #000;
}
.details_text .shopName,
.address{
    color: #5f5f5f;
}
.titleName{
  font-size: 32rpx;
}
.fraction .fraction_text{
    text-align: center;
    margin-bottom: 5px;
    font-size: 18px;
    color: #eeb085;
}
.fraction .fraction_img{
    width: 70rpx;
    height: 70rpx;
    padding:10rpx;
    border-radius: 50%;
    border: 1px solid #ccc;
    box-sizing: border-box;
}
.close{
    width: 30px;
    height: 30px;
}

image{
    width: 100%;
    height: 100%;
}
.tabs_box{
    margin-top: 10px;
    display: flex;
    flex-wrap: wrap;
}
.tabs_item{
    padding: 3px 10px;
    margin-right: 20rpx;
    border: 1px solid #eeb085;
    border-radius: 10rpx;
}